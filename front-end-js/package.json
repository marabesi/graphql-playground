{
  "name": "front-end-js",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "dotenv": "^8.2.0",
    "get-graphql-schema": "^2.1.2",
    "graphql": "^14.5.8",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-relay": "^8.0.0",
    "react-scripts": "3.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "export-schema": "get-graphql-schema http://localhost:4002 > schema.graphql",
    "relay-compile": "relay-compiler --src ./src --schema ./schema.graphql",
    "relay": "yarn export-schema && yarn relay-compile"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-plugin-relay": "^8.0.0",
    "relay-compiler": "^8.0.0"
  }
}
